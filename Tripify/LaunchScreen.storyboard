<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<document type="com.apple.InterfaceBuilder3.CocoaTouch.Storyboard.XIB" version="3.0" toolsVersion="18122" targetRuntime="iOS.CocoaTouch" propertyAccessControl="none" useAutolayout="YES" initialViewController="LaunchScreen">
    <scenes>
        <scene sceneID="TripifyLaunch">
            <objects>
                <viewController id="LaunchScreen" sceneMemberID="viewController">
                    <view key="view" contentMode="scaleToFill" id="launchView" backgroundColor="systemBackgroundColor">
                        <subviews>
                            <!-- App Logo -->
                            <imageView contentMode="scaleAspectFit" image="AppLogo" translatesAutoresizingMaskIntoConstraints="NO" id="logoView">
                                <rect key="frame" x="0.0" y="0.0" width="200" height="200"/>
                            </imageView>

                            <!-- Main Title -->
                            <label text="TripiFy" textAlignment="center" translatesAutoresizingMaskIntoConstraints="NO" id="titleLabel">
                                <fontDescription type="system" pointSize="28" weight="semibold"/>
                                <color key="textColor" name="labelColor"/>
                            </label>

                            <!-- Subtitle -->
                            <label text="Project by Krishay Gahlaut" textAlignment="center" translatesAutoresizingMaskIntoConstraints="NO" id="subtitleLabel">
                                <fontDescription type="system" pointSize="16"/>
                                <color key="textColor" name="secondaryLabelColor"/>
                            </label>
                        </subviews>
                        <constraints>
                            <constraint firstItem="logoView" firstAttribute="centerX" secondItem="launchView" secondAttribute="centerX"/>
                            <constraint firstItem="logoView" firstAttribute="centerY" secondItem="launchView" secondAttribute="centerY" constant="-80"/>
                            <constraint firstItem="titleLabel" firstAttribute="top" secondItem="logoView" secondAttribute="bottom" constant="20"/>
                            <constraint firstItem="titleLabel" firstAttribute="centerX" secondItem="launchView" secondAttribute="centerX"/>
                            <constraint firstItem="subtitleLabel" firstAttribute="top" secondItem="titleLabel" secondAttribute="bottom" constant="8"/>
                            <constraint firstItem="subtitleLabel" firstAttribute="centerX" secondItem="launchView" secondAttribute="centerX"/>
                        </constraints>
                        <viewLayoutGuide key="safeArea" id="safeArea"/>
                    </view>
                </viewController>
                <placeholder placeholderIdentifier="IBFirstResponder" id="responder" userLabel="First Responder" sceneMemberID="firstResponder"/>
            </objects>
        </scene>
    </scenes>
</document>
